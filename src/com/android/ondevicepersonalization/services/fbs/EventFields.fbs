namespace com.android.ondevicepersonalization.services.fbs;

table EventFields {
    data:KeyValueList;
}

table KeyValueList {
    entries:[KeyValue];
}

table KeyValue {
    key:string;
    type:byte;
    byte_value:byte;
    short_value:short;
    int_value:int;
    long_value:long;
    float_value:float;
    double_value:double;
    string_value:string;
    blob_value:[byte];
    bool_value:bool;
}

root_type EventFields;
