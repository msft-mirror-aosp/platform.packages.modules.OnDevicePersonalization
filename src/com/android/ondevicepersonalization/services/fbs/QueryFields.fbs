include "EventFields.fbs";

namespace com.android.ondevicepersonalization.services.fbs;

table QueryFields {
    slots:[Slot];
}

table Slot {
    id:string;
    winning_bids:[Bid];
    rejected_bids:[Bid];
}

table Bid {
    id:string;
    price:double;
    score:double;
    metrics:Metrics;
}

root_type QueryFields;